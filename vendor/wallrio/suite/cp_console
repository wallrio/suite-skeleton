#!/usr/bin/env php
<?php	
/*
 * Suite Framework
 * ================
 * suite.wallrio.com
 *
 * This file is part of the Suite Core.
 *
 * Wallace Rio <wallacerio@wallrio.com>
 *
 * ------------------------------------
 * 
 * This file has generated by setup component 
 * 
 */	

	
	$path = dirname(__FILE__);
	define('console_path',$path);
	

	if (version_compare(phpversion(), '5.3.0', '<'))
		require console_path.'/libs/wallrio/suite/Suite.php';
	else
		require console_path.'/vendor/autoload.php';

		
	if(!Suite::component('setup','checkexist')){
		echo "\n\n".'Component "setup" not found'."\n\n";
		exit;
	}

	$result = Suite::component('setup',array(
		'exclude' => 'plugins,modules'
	))->console();

	echo $result;

